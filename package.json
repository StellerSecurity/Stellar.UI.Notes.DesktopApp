{
  "name": "stellar-notes",
  "productName": "Stellar Notes",
  "version": "0.0.1",
  "author": {
    "name": "Stellar Security",
    "email": "support@stellarsecurity.com"
  },
  "homepage": "https://stellarsecurity.com/",
  "private": true,
  "description": "Stellar Private Notes",
  "main": "electron-start.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "electron": "electron .",
    "electron:macBuild": "npm run build && electron-builder --mac",
    "electron:winBuild": "npm run build && electron-builder --win",
    "electron:linuxBuild": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@capacitor/android": "5.2.3",
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "5.2.3",
    "@capacitor/filesystem": "^7.1.4",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/network": "^7.0.2",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/share": "^7.0.1",
    "@capacitor/status-bar": "^5.0.6",
    "@ionic/angular": "^7.0.0",
    "@ionic/storage-angular": "^4.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@stellarsecurity/stellar-crypto": "^1.0.2",
    "@wfpena/angular-wysiwyg": "^1.1.3",
    "@zxcvbn-ts/core": "^2.2.1",
    "@zxcvbn-ts/language-en": "^2.1.0",
    "angular-froala-wysiwyg": "^4.2.0",
    "angular-password-strength-meter": "^6.0.0",
    "capacitor-secure-storage-plugin": "^0.9.0",
    "crypto-browserify": "^3.12.1",
    "crypto-js": "^4.1.1",
    "electron-updater": "^6.6.2",
    "ionicons": "^7.0.0",
    "js-sha512": "^0.9.0",
    "js-yaml": "^4.1.1",
    "libsodium-wrappers-sumo": "^0.8.2",
    "ng-otp-input": "^2.0.8",
    "ngx-toastr": "^19.1.0",
    "rxjs": "~7.8.0",
    "timestamp-to-date": "^1.1.0",
    "tslib": "^2.3.0",
    "unix-timestamp": "^1.0.3",
    "uuid": "^9.0.1",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0",
    "@angular-eslint/builder": "^16.0.0",
    "@angular-eslint/eslint-plugin": "^16.0.0",
    "@angular-eslint/eslint-plugin-template": "^16.0.0",
    "@angular-eslint/schematics": "^16.0.0",
    "@angular-eslint/template-parser": "^16.0.0",
    "@angular/cli": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@angular/language-service": "^16.0.0",
    "@capacitor-community/electron": "^5.0.1",
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "5.2.3",
    "@ionic/angular-toolkit": "^9.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jasmine": "~4.3.0",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "electron": "^36.3.1",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~4.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "rollup": "^4.57.1",
    "ts-node": "^8.3.0",
    "typescript": "~5.0.2"
  },
  "build": {
    "appId": "stellar.notes.app",
    "productName": "Stellar Notes",
    "files": [
      "dist/StellerPhoneNotesApp/**/*",
      "electron-start.js",
      "preload.js",
      "package.json"
    ],
    "directories": {
      "buildResources": "icons"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://desktopreleasesassetsprod.stellarsecurity.com/notes/linux/"
      }
    ],
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility",
      "maintainer": "Stellar Security <support@stellarsecurity.com>",
      "executableName": "stellar-notes",
      "synopsis": "Private, end-to-end encrypted notes.",
      "icon": "icons/linux",
      "artifactName": "${productName}-${version}.${ext}",
      "desktop": {
        "entry": {
          "Name": "Stellar Notes",
          "Comment": "Stellar Private Notes",
          "Categories": "Utility;Security;",
          "Keywords": "notes;privacy;security;encryption;",
          "StartupWMClass": "stellar-notes"
        }
      }
    },
    "mac": {
      "icon": "icons/icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "icons/icon.png"
    }
  }
}
